<br />
<table width="100%" align="center" valign="left">
	<tr>
		<th width="100%" align="center" valign="middle">{L_PAGE_TITLE}</th>
	</tr>
</table>
<table width="100%" align="top" class="forumline">
	<tr>
		<th width="5%" align="center" valign="middle">&nbsp;</th>
		<th width="15%" align="center" valign="middle">{L_CHEAT_IP}</th>
		<th width="20%" align="center" valign="middle">{L_CHEAT_USERNAME}</th>
		<th width="20%" align="center" valign="middle">{L_CHEAT_CHARACTER}</th>
		<th width="{L_WIDTH}%" align="center" valign="middle">{L_CHEAT_TYPE}</th>
		<th width="15%" align="center" valign="middle">{L_CHEAT_DATE}</th>
<!-- BEGIN moderate -->
		<th width="5%" align="center" valign="middle">{L_CHEAT_ACTION}</th>
<!-- END moderate -->
	</tr>
<!-- BEGIN rows -->
	<tr>
		<td align="left" valign="middle" class="{rows.ROWS}"><span class="genmed">{rows.NUM}</span></td>
		<td align="center" valign="middle" class="{rows.ROWS}"><span class="genmed">{rows.CHEAT_IP}</span></td>
		<td align="left" valign="middle" class="{rows.ROWS}"><span class="genmed"><a href="{rows.U_CHEAT_USERNAME}" title="{L_CHEAT_PROFILE}">{rows.CHEAT_USERNAME}</a></span></td>
		<td align="left" valign="middle" class="{rows.ROWS}"><span class="genmed"><a href="{rows.U_CHEAT_CHARACTER}" title="{L_CHARACTER}">{rows.CHEAT_CHARACTER}</a></span></td>
		<td align="left" valign="middle" class="{rows.ROWS}" onMouseOver="this.style.backgroundColor=''; this.style.cursor='arrow';" onMouseOut=this.style.backgroundColor="" title="{rows.CHEAT_PUNISHMENT}" ><span class="genmed">{rows.CHEAT_TYPE}</span></td>
		<td align="center" valign="middle" class="{rows.ROWS}"><span class="genmed">{rows.CHEAT_DATE}</span></td>
<!-- BEGIN moderate -->
		<td align="center" valign="middle" class="{rows.ROWS}"><span class="gensmall"><a href="{rows.moderate.PUBLIC_LINK}" title="Hide/Unhide this entry">{rows.moderate.PUBLIC}</a>&nbsp;&nbsp;&nbsp;<a href="{rows.moderate.CHEAT_MOD_LINK}" title="Imprison or Ban this user">{rows.moderate.MOD}</a></span></td>
<!-- END moderate -->
	</tr>
<!-- END rows -->
</table>
<table width="100%" align="center" valign="middle" class="forumline">
	<tr>
		<th width="100%">&nbsp;</th>
	</tr>
</table>
<table width="100%" align="center" valign="middle">
	<tr>
		<td><span class="genmed">{PAGE_NUMBER}</span></td>
		<td align="right"><span class="genmed">{PAGINATION}</span></td>
	</tr>
</table>
<table width="100%" align="center" valign="middle" border="0" >
	<tr>
		<td align="center"><span class="gensmall">{L_PAGE_NOTE}</span></td>
	</tr>
</table>
<br clear="all" />

